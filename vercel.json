{
  "version": 2,
  "builds": [
    {
      "src": "build.js",
      "use": "@vercel/node"
    },
    {
      "src": "Backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "Frontend/dist",
  "routes": [
    { "src": "/api/(.*)", "dest": "Backend/server.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}